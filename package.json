{
  "name": "gdlauncher",
  "version": "1.1.30",
  "description": "GDLauncher is simple, yet powerful Minecraft custom launcher with a strong focus on the user experience",
  "keywords": [
    "minecraft",
    "launcher",
    "gaming",
    "games",
    "react",
    "redux",
    "reload",
    "sass",
    "webpack"
  ],
  "bugs": {
    "url": "https://github.com/gorilla-devs/GDLauncher/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gorilla-devs/GDLauncher.git"
  },
  "license": "GPL-3.0",
  "author": {
    "name": "GorillaDevs",
    "email": "davide@gdevs.io",
    "url": "https://gdevs.io"
  },
  "private": false,
  "homepage": "./",
  "main": "build/electron/main.js",
  "scripts": {
    "install": "npm run napi-build",
    "prepare": "husky install && npm run nsfw-build",
    "dev": "concurrently \"npm run start-web:dev\" \"npm run build-electron:dev && npm run start-electron:dev\"",
    "release": "npm run napi-build && rimraf ./deploy && npm run release:setup && npm run release:portable",
    "lint": "eslint .",
    "napi-build": "cd napi && npm run build && node ../scripts/moveNapi.js",
    "nsfw-build": "cd node_modules/nsfw && node-gyp build  && cd ../.. && node scripts/moveNsfw.js",
    "start-electron:dev": "cross-env-shell NODE_ENV=development APP_TYPE=electron BROWSER=none wait-on http://localhost:8080 -i 2000 && electron .",
    "start-electron:prod": "electron .",
    "start-web:dev": "cross-env-shell REACT_APP_RELEASE_TYPE=setup webpack serve --hot --mode development",
    "build-web:setup": "cross-env-shell REACT_APP_RELEASE_TYPE=setup webpack --mode production",
    "build-web:portable": "cross-env-shell REACT_APP_RELEASE_TYPE=portable webpack --mode production",
    "build-electron:dev": "cross-env-shell REACT_APP_RELEASE_TYPE=setup webpack --mode development --config electron.config.webpack.js",
    "build-electron:setup": "cross-env-shell REACT_APP_RELEASE_TYPE=setup webpack --mode production --config electron.config.webpack.js",
    "build-electron:portable": "cross-env-shell REACT_APP_RELEASE_TYPE=portable webpack --mode production --config electron.config.webpack.js",
    "release:setup": "npm run build-web:setup && npm run build-electron:setup && npm run deploy setup",
    "release:portable": "npm run build-web:portable && npm run build-electron:portable && npm run deploy portable",
    "deploy": "node ./scripts/createDeploy.js",
    "upload": "node ./scripts/uploadRelease.js"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "cross-env NODE_ENV=development eslint --cache --format=pretty",
      "prettier --ignore-path .eslintignore --single-quote --write",
      "git add"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc,stylelintrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint --ignore-path .eslintignore --syntax scss --fix",
      "prettier --ignore-path .eslintignore --single-quote --write",
      "git add"
    ],
    "*.{yml,md}": [
      "prettier --ignore-path .eslintignore --single-quote --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=14.16.0"
  },
  "cpu": [
    "x64",
    "arm64",
    "arm"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
